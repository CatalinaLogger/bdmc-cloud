//to reset element-ui default css
.el-upload
  input[type="file"]
    display none !important

.el-upload__input
  display none

//暂时性解决diolag 问题 https://github.com/ElemeFE/element/issues/2461
.el-dialog
  transform none
  left 0
  position relative
  margin 0 auto

//element ui upload
.upload-container
  .el-upload
    width 100%
    .el-upload-dragger
      width 100%
      height 200px

.el-dialog__body
  padding 10px 20px 0 20px

.el-dialog__wrapper
  display flex
  justify-content center
  align-items Center

.el-table
  .two-row
    color #e6a23c
  .one-row
    color: #f56c6c
  .four-row
    color: #909399
  .five-row
    color: #5dab33



.chart-detail
  .el-dialog__body
    padding 0
    overflow-x hidden

.el-menu--horizontal
  border-bottom none
.el-menu--horizontal>.el-menu-item
  height 50px
  line-height 50px

.image-container
  .el-dialog__header
    padding 0
  .el-dialog__body
    padding 0

